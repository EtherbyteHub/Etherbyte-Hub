    [16981421605] = "https://raw.githubusercontent.com/EtherbyteHub/reborn-as-a-swords-man-key/main/reborn%20as%20a%20swords%20man%20key", -- Reborn As Swordsman
    [15154513235] = "https://raw.githubusercontent.com/EtherbyteHub/kamehameha-simulator-key/main/kamehameha%20simulator%20key", -- Kamehameha Simulator
